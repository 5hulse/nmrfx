<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<BorderPane id="AnchorPane" prefHeight="200.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nmrfx.analyst.gui.git.GitHistoryController">
    <stylesheets>
        <URL value="@/styles/consolescene.css"/>
    </stylesheets>
    <top>
        <VBox spacing="6">
            <Button text="Commit" onAction="#gitCommit"/>
            <HBox spacing="6">
                <Label text="Commit/Branch Actions:"/>
                <MenuButton fx:id="actionMenu" text="Action"/>
            </HBox>
        </VBox>
    </top>
    <center>
        <TableView fx:id="historyTable" prefHeight="800" minHeight="400.0" prefWidth="600" VBox.vgrow="ALWAYS"/>
    </center>
</BorderPane>
