<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<HBox prefHeight="600.0" prefWidth="900.0" styleClass="mainFxmlClass" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.nmrfx.processor.gui.FXMLController">
    <children>
        <BorderPane fx:id="mainBox" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
            <stylesheets>
                <URL value="@/styles/Styles.css"/>
            </stylesheets>
            <top>
                <VBox fx:id="topBar" GridPane.hgrow="ALWAYS">
                    <children>
                        <ToolBar fx:id="toolBar" prefWidth="200.0" BorderPane.alignment="CENTER">
                            <items>
                            </items>
                        </ToolBar>
                    </children>
                </VBox>
            </top>
            <center>
                <BorderPane fx:id="borderPane" HBox.hgrow="ALWAYS">
                    <top>
                    </top>
                    <center>
                        <StackPane fx:id="chartPane" prefHeight="1000" VBox.vgrow="ALWAYS"/>
                    </center>
                    <right>
                    </right>
                </BorderPane>
            </center>
            <right>
            </right>
            <bottom>
                <VBox fx:id="bottomBox">
                    <children>
                        <VBox fx:id="btoolVBox">
                            <children>
                                <Pane HBox.hgrow="ALWAYS"/>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </bottom>
        </BorderPane>
        <Separator orientation="VERTICAL"/>
        <StackPane fx:id="rightContentPane" VBox.vgrow="ALWAYS"/>
    </children>

</HBox>
